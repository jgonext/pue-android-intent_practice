<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="es.pue.intentpractice.presentationlayer.controllers.activities.WebActivity">

    <Button
        android:id="@+id/aw_btn_browser"
        android:layout_width="302dp"
        android:layout_height="52dp"
        android:layout_marginTop="44dp"
        android:text="@string/aw_btn_browser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aw_txt" />

    <Button
        android:id="@+id/aw_btn_search"
        android:layout_width="302dp"
        android:layout_height="52dp"
        android:layout_marginTop="36dp"
        android:text="@string/aw_btn_search"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aw_btn_browser"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/aw_txt"
        android:layout_width="302dp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/aw_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>